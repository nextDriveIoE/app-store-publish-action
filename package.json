{
  "name": "app-store-publish-sdk",
  "version": "1.0.0",
  "description": "自動 App Store 送審專案 SDK",
  "main": "dist/action.js",
  "types": "dist/action.d.ts",
  "scripts": {
    "build": "tsc",
    "build:action": "tsc && cp src/action.ts dist/ && node -e \"require('fs').writeFileSync('dist/action.js', require('fs').readFileSync('dist/action.js', 'utf8'))\"",
    "prepare": "npm run build",
    "package": "npm run build && npm prune --production"
  },
  "keywords": [
    "app-store",
    "ios",
    "appstore-connect",
    "automated-publishing",
    "nextdrive"
  ],
  "author": "NextDrive",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "appstore-connect-sdk": "^1.3.0",
    "axios": "^1.0.0",
    "dotenv": "^16.0.0"
  }
}
